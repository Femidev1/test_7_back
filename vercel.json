{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "index.js"
    }
  ],
  "functions": {
    "index.js": {
      "runtime": "nodejs18.x",
      "memory": 512,
      "maxDuration": 10
    }
  },
  "env": {
    "NODE_ENV": "production",
    "MONGO_URI": "@mongo_uri",
    "PORT": "5050"
  }
}